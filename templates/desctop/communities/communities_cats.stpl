<label style="margin-top: 12px;">Подкатегория</label>
<select class="form-control" name="category" id="id_category" data-live-search="true" tabindex="-1" aria-hidden="true">
   <% for object in categ.iter() { %>
      <option value="<%= object.id %>"><%= object.name %></option>
   <% } %>
</select>
